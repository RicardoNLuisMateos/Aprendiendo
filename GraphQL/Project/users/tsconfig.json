{
    "compilerOptions": {
      "target": "ES6",
      "outDir": "./build/",
      "module": "commonjs",
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "esModuleInterop": true
    },
    "exclude": ["node_modules", "notes.txt", "README.md", "package.json", "package-lock.json", "tsconfig.json",".env", ".gitignore"]
  }